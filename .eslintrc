{
  "extends": ["next", "prettier", "plugin:tailwind/recommended"],
  "rules": {
    "@next/next/no-page-custom-font": "off",
    "react/no-unescaped-entities": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-var": "off",
    "tailwind/class-order": "off",
    "react/display-name": "warn"
  }
}
